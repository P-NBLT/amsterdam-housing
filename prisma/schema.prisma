// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model houseDetails {
  id Int @id @default(autoincrement())
  address String
  postcalCode String
  price String
  size String
  rooms String
  availability String
  realEstate String
  coordinates String
  image String
}
